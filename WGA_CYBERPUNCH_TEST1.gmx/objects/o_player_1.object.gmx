            <string>///pos
draw_set_color(color);
switch statuspos 
{
    case 'sit':
    {
        if keyboard_check(vk_down)
        {
            draw_rectangle(x,y,x+128,y-128,1);
        }
        else
        {
            statuspos = 'none';   
        }
        break;        
    }
    case 'jump':
    {
        draw_rectangle(x,y,x+128,y-256,1);
        if y == ystart
        {
            vspeed = jump_speed
        }
        break;        
    }
    case 'left':
    {
        draw_rectangle(x,y,x+128,y-256,1);
        if x &gt; 0
        {
            x -= move_speed
        }
        else
        {
            x = 0
        }
        if !keyboard_check(vk_left)
        {
            statuspos = 'none'    
        }
        break;        
    }
    case 'right':
    {
        draw_rectangle(x,y,x+128,y-256,1);
        if x &lt; room_width-128
        {
            x += move_speed
        }
        else
        {
            x = room_width-128
        }
        if !keyboard_check(vk_right)
        {
            statuspos = 'none'    
        }
        break;        
    }
    default:
    {   
        draw_rectangle(x,y,x+128,y-256,1);
        break;
    }
}

//gravity
